  <xs:simpleType name="WKT-SWEREF99TM-3D">
    <xs:annotation>
      <xs:documentation xml:lang="en"> Specifies a well known text in SWEREF99 format. The
        coordinate order is in X Y Z, where Z is optional. </xs:documentation>
      <xs:documentation xml:lang="sv"> Anger en well known text i SWEREF99-format. Koordinaternas
        ordning anges i X Y Z, där Z är frivillig. </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(POINT|LINESTRING|MULTILINESTRING|POLYGON|MULTIPOLYGON)\s?\(.*\)" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="WKT-WGS84-3D">
    <xs:annotation>
